{
    "include": [
        "./types/*.d.ts"
    ],
    "files": [
        "src/manglr.ts"
    ],
    "compilerOptions": {
        "inlineSourceMap": true,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "target": "es6",
        "module": "commonjs",
        "lib": ["es6"],
        "outDir": "lib",
        "pretty": true
    }
}
